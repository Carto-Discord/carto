{
  "name": "carto-receiver",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/index.js",
  "scripts": {
    "prebuild": "rimraf build/",
    "build": "tsc",
    "build:package": "copyfiles build/**/* package.json package-lock.json package",
    "start": "functions-framework --source=build/src/ --target=receiver --signature-type=event",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --collectCoverage",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\""
  },
  "keywords": [],
  "author": "Tohaker",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@google-cloud/storage": "^5.8.5",
    "node-fetch": "^2.6.1",
    "slash-commands": "^1.4.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "concurrently": "^6.1.0",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6"
  }
}
