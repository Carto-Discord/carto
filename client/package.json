{
  "name": "carto-client",
  "version": "0.1.0",
  "main": "build/src/index.js",
  "scripts": {
    "prebuild": "rimraf build/",
    "postbuild": "copyfiles .env build/",
    "build": "tsc",
    "build:package": "copyfiles build/**/* package.json package-lock.json package",
    "register": "node ./build/src/registerCommands.js",
    "start": "functions-framework --source=build/src/ --target=slashFunction",
    "test": "jest --watch",
    "test:ci": "jest",
    "test:coverage": "jest --collectCoverage",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\""
  },
  "author": "Tohaker",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^4.11.0",
    "@google-cloud/storage": "^5.8.5",
    "google-auth-library": "^7.0.4",
    "node-fetch": "^2.6.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.7.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "concurrently": "^6.0.2",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "slash-commands": "^1.4.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
