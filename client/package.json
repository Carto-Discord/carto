    "@types/aws-lambda": "8.10.84",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "@types/node-fetch": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "concurrently": "7.0.0",
    "copyfiles": "2.4.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
